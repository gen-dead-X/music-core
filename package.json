{
  "name": "type-graphql-demo",
  "version": "1.0.0",
  "description": "TypeGraphQL Project",
  "main": "server.ts",
  "scripts": {
    "tsc": "tsc",
    "start": "npm run tsc && node ./dist/server.js",
    "dev": "nodemon src/server.ts",
    "dev:dist": "nodemon dist/server.js",
    "browser:debug": "nodemon --inspect=9230 dist/server.js",
    "prisma:generate": "yarn prisma generate",
    "prisma:migrate": "yarn prisma migrate dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "npm run clean:build && npm run check:version",
    "build": "npx tsc --build ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.typings.json",
    "postbuild": "npx shx rm ./build/typings/shim.d.ts && npx shx cp ./src/shim.ts ./build/typings && npx ts-node ./scripts/package.json.ts",
    "prebuild:benchmarks": "npm run clean:build:benchmarks",
    "build:benchmarks": "npx tsc --build ./benchmarks/tsconfig.json",
    "release": "npm version --message \"release: %s\"",
    "version": "npm run --prefix ./website new-release --release=$npm_package_version && git add -A .",
    "docs": "npm run start --prefix website",
    "check:format": "npx prettier --check .",
    "check:lint": "npx eslint .",
    "fix": "npx npm-run-all --npm-path npm \"fix:*\"",
    "fix:format": "npx prettier --write .",
    "fix:lint": "npx eslint --fix .",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "fix:markdown": "npx markdownlint --fix \"**/*.md\"",
    "gen:docs": "npx ts-node ./scripts/markdown.ts --on docs",
    "gen:readme": "npx ts-node ./scripts/markdown.ts --on readme"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "reflect-metadata": "^0.2.1",
    "type-graphql": "2.0.0-beta.3",
    "@prisma/client": "^5.7.1",
    "@types/jsonwebtoken": "^9.0.5",
    "bcryptjs": "^2.4.3",
    "@apollo/client": "^3.8.8",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "date-fns": "^2.30.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.1",
    "pg": "^8.11.3",
    "prisma": "^5.7.0",
    "randomstring": "^1.3.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.4",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "bun-types": "latest",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1"
  },
  "peerDependencies": {
    "class-validator": ">=0.14.0",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4"
  },
  "peerDependenciesMeta": {
    "class-validator": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 18.0.0"
  }
}
